{
  "@context": "http://www.w3.org/ns/csvw",
  "url": "./walmart_products.csv",
  "tableSchema": {
      "aboutUrl": "http://recipes-project.com/products/{index}",
      "columns": [
            {
                "titles": ["index"],
                "name": "index",
                "required": true,
                "propertyUrl": "schema:index",
                "suppressOutput": true
            },
            {
                "titles": ["product_name"],
                "name": "name",
                "propertyUrl": "http://recipes-project.com/schema/productName"
            },
            {
                "titles": ["brand"],
                "name": "brand",
                "propertyUrl": "http://recipes-project.com/schema/brandName"
            },
            {
                "titles": ["category"],
                "name": "category",
                "propertyUrl": "http://recipes-project.com/schema/categoryName"
            },
            {
                "titles": ["price_current"],
                "datatype": "string",
                "propertyUrl": "http://recipes-project.com/schema/price"
            }
      ]
  }
}
