@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix : <http://recipes-project.com/schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix shapes: <http://recipes-project.com/shapes#> .

shapes:brand a sh:NodeShape ;
  sh:targetClass :Brand ;
  sh:property [
    sh:path :brandName ;
    sh:datatype xsd:string;
    sh:minLength 1;
    sh:minCount 1 ;
    sh:maxCount 1
  ] .
